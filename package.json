{
  "name": "@artdon/ownlibrary",
  "version": "0.0.3",
  "description": "\"# My Own Library\"",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version_": "npm run format && git add -A src",
    "postversion_": "git push && git push --tags",
    "publish_": "npm publish --access public",
    "v_0": "node ./utils/setVersion.js 0",
    "v_1": "node ./utils/setVersion.js 1",
    "v_2": "node ./utils/setVersion.js 2",
    "v_3": "node ./utils/setVersion.js 3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtDon/ownLibrary.git"
  },
  "keywords": [
    "custom libarary",
    "own npm module"
  ],
  "author": "ArtDon adkcenter@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArtDon/ownLibrary/issues"
  },
  "homepage": "https://github.com/ArtDon/ownLibrary#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "cli-color": "^2.0.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "request": "^2.88.2"
  }
}
